{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "docs/CHANGELOG.md"
      }
    ],
    "@semantic-release/npm",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/main.js",
            "label": "CommonJS distribution",
            "name": "chartjs-plugin-tailwindcss-colors-v${nextRelease.gitTag}.js"
          },
          {
            "path": "dist/main.esm.js",
            "label": "ESM distribution",
            "name": "chartjs-plugin-tailwindcss-colors-v${nextRelease.gitTag}.esm.js"
          },
          {
            "path": "dist/types.d.ts",
            "label": "Types"
          }
        ]
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "yarn.lock",
          "docs/CHANGELOG.md"
        ],
        "message": "chore(release): v${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}
