{
  "name": "chartjs-plugin-tailwindcss-colors",
  "version": "0.2.0",
  "author": "Danry Parens Patriae Ague <ague.danryparenspatriae@gmail.com>",
  "description": "Colorize your Chart.js components using TailwindCSS!",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/plugin.umd.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production yarn build",
    "watch": "rollup -c --configPlugin rollup-plugin-ts -w",
    "dev": "conc -n R,J -c red,magenta -k \"yarn watch\" \"yarn test:dev\"",
    "build": "rollup -c --configPlugin rollup-plugin-ts",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "format:checkonly": "prettier . -c",
    "format": "prettier . -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylecheck": "yarn format:checkonly && yarn lint",
    "stylefix": "yarn format && yarn lint:fix"
  },
  "homepage": "https://github.com/decanTyme/chartjs-plugin-tailwindcss-colors#readme",
  "bugs": "https://github.com/decanTyme/chartjs-plugin-tailwindcss-colors/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/decanTyme/chartjs-plugin-tailwindcss-colors"
  },
  "keywords": [
    "chartjs",
    "chart.js",
    "tailwindcss",
    "colors",
    "class",
    "plugin",
    "parser"
  ],
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js",
    "browser": "./dist/plugin.umd.js",
    "default": "./dist/index.esm.js",
    "types": "./dist/types.d.ts"
  },
  "dependencies": {
    "color-name": "^1.1.4",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@kayahr/jest-electron-runner": "^29.11.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@tsconfig/recommended": "^1.0.3",
    "@types/color-name": "^1.1.3",
    "@types/jest": "^29.5.11",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.set": "^4.3.9",
    "@types/pixelmatch": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "chart.js": "^3.9.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.7.0",
    "pixelmatch": "^5.3.0",
    "prettier": "^2.8.8",
    "rollup": "^4.9.5",
    "rollup-plugin-ts": "^3.4.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "chart.js": "^3.0.0",
    "tailwindcss": "^3.0.0"
  }
}
